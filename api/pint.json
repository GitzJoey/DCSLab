{
    "preset": "laravel",
    "rules": {
        "unary_operator_spaces": false,
        "not_operator_with_successor_space": false,
        "no_unused_imports": false
    },
    "exclude": [
        "database/migrations"
    ],
    "notName": [
        "*Test.php"
    ],
    "notPath": [
        "config/laratrust.php",
        "config/laratrust_seeder.php",
        "config/image.php",
        "database/seeders/LaratrustSeeder.php"
    ]
}