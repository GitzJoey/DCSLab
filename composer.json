{
    "name": "gitzjoey/dcslab",
    "type": "project",
    "description": "DCSLab - Laravel Always Updated ERP System",
    "keywords": ["ERP", "framework", "laravel", "gitzjoey"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "cmgmyr/messenger": "^2.27",
        "doctrine/dbal": "^3.3",
        "fico7489/laravel-pivot": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laravel/fortify": "^1.11",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mews/captcha": "^3.2",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^7.0",
        "santigarcor/laratrust": "^7.0",
        "spatie/laravel-activitylog": "^4.4",
        "tightenco/ziggy": "^1.4",
        "vinkla/hashids": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Unit\\Service\\": "tests/Unit/Service/",
            "Tests\\Feature\\API\\": "tests/Feature/API/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "artisan_xdebug": "@php -dxdebug.start_with_request=yes -dxdebug.mode=debug -dxdebug.client_port=9003 -dxdebug.client_host=127.0.0.1 artisan"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
